{
    "workbench.colorTheme": "Adapta Nokto",
    "editor.fontFamily": "'Cascadia Code', 'Courier New', monospace",
    // "workbench.colorTheme": "Monokai Pro (Filter Octagon)",
    // "editor.fontFamily": "'Jetbrains Mono', 'Courier New', monospace",
    // "editor.fontFamily": "'Fira Code iScript', 'monospace', monospace",
    // "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    "editor.fontLigatures": true,
    "editor.fontWeight": 600,
    "editor.fontSize": 16,
    "editor.cursorBlinking": "expand",
    "editor.cursorStyle": "block",
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#2be09e",
        "terminalCursor.foreground": "#2be09e"
    },
    "terminal.integrated.fontSize": 16,
    // "[javscript]":{
    // },
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    //following will be in italic (=FlottFlott)
                    "comment",
                    "entity.name.type.class", //class names
                    "keyword", //import, export, returnâ€¦
                    "storage.modifier", //static keyword
                    // "storage.type", //class keyword
                    "storage.type.class",
                    "support.class.builtin",
                    "keyword.control",
                    "constant.language",
                    "entity.other.attribute-name",
                    "string.quoted.single",
                    "entity.name.method",
                    "entity.name.tag",
                    "variable.language.this.php",
                    "support.type.primitive.ts",
                    "variable.language.this.ts",
                    "variable.language.super.ts",
                    "keyword.operator.comparison",
                    "keyword.operator.class.php",
                    "keyword.operator.comparison.php"
                ],
                "settings": {
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": ["constant"],
                "settings": {
                    "foreground": "#2be09e"
                }
            },
            {
                "scope": ["variable.parameter"],
                "settings": {
                    "foreground": "#dd6986"
                }
            },
            {
                "scope": [
                    "variable.other.constant",
                    "variable.other.readwrite",
                    "variable.other.property",
                    "variable.other.object.property.ts"
                ],
                "settings": {
                    "foreground": "#e96daf"
                }
            },
            {
                "scope": [
                    "variable.other.object.property.ts",
                    "variable.other.object.property.js"
                ],
                "settings": {
                    "foreground": "#55c9d8"
                }
            },
            {
                "scope": ["variable.other.object.ts"],
                "settings": {
                    "foreground": "#80dfa8"
                }
            },
            {
                "scope": [
                    "punctuation.definition.variable.php",
                    "variable.other.object.js"
                ],
                "settings": {
                    "foreground": "#41e59b"
                }
            },
            {
                "scope": [
                    "keyword.other.DDL.create.II.sql"
                ],
                "settings": {
                    "foreground": "#41e2e5"
                }
            },
            {
                "scope": ["keyword.other.phpdoc.php"],
                "settings": {
                    "foreground": "#e776b6"
                }
            },
            {
                "scope": ["keyword.other.sql"],
                "settings": {
                    "foreground": "#e776b6"
                }
            },
            {
                "scope": ["support.attribute.php"],
                "settings": {
                    "foreground": "#41e59b"
                }
            },
            {
                "scope": ["meta.attribute.php"],
                "settings": {
                    "foreground": "#e776b6"
                }
            },
            {
                "scope": ["keyword.other.data-integrity.sql"],
                "settings": {
                    "foreground": "#bc92df"
                }
            },
            {
                "scope": [
                    "keyword.other.type.php",
                    "variable.other.global.php"
                ],
                "settings": {
                    "foreground": "#8d6fd3"
                }
            },
            {
                "scope": [
                    "keyword.other.DML.sql"
                ],
                "settings": {
                    "foreground": "#8d6fd3"
                }
            },
            {
                "scope": ["constant.language.php"],
                "settings": {
                    "foreground": "#68f073"
                }
            },
            {
                "scope": ["variable.language.this.php"],
                "settings": {
                    "foreground": "#e2407b"
                }
            },
            {
                "scope": ["constant.numeric.sql"],
                "settings": {
                    "foreground": "#e1608b"
                }
            },
            {
                "scope": ["variable.other.php"],
                "settings": {
                    "foreground": "#f2edef"
                }
            }
        ],
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        // "editor.formatOnSave": true
    },
    // // "prettier.jsxSingleQuote": true,
    // // "prettier.singleQuote": true,
